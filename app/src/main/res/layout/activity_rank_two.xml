<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.RankTwoActivity">
    <include
        android:id="@+id/toolbar_rankTwo"
        layout="@layout/toolbar_main"/>
    <com.google.android.material.tabs.TabLayout
        android:id="@+id/rankTwo_tabLayout"
        android:layout_below="@id/toolbar_rankTwo"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#FCFCFC"
        app:tabIndicatorColor="#5DCAFF"/>
    <androidx.viewpager.widget.ViewPager
        android:id="@+id/rankTwo_viewPage"
        android:layout_below="@id/rankTwo_tabLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

</RelativeLayout>